{
  "testEnvironment": "jsdom",
  "clearMocks": true,
  "coverageDirectory": "<rootDir>/../../test/unionpay/coverage",
  "displayName": "unionpay",
  "restoreMocks": true,
  "//": "In the future, tests should live in the same directory as the files they are testing",
  "rootDir": "../../src/unionpay",
  "roots": ["<rootDir>", "<rootDir>/../../test/unionpay"],
  "setupFilesAfterEnv": ["<rootDir>/../../test/unionpay/unit/global.js"],
  "testMatch": ["**/unit/**/*.js", "!**/global.js"]
}
